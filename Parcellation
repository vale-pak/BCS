# select each parcell
cd ROIs2mm
for a in *.nii.gz
do echo $a
#select each image to parcell
cd ..
    cd Images_zmaps_thr
    for b in *.nii.gz
    do echo $b
    cd ..
# compute average z for voxels within the parcell 
fslstats Images_zmaps_thr/${b} -k ROIs2mm/${a} -m>>$a.csv
    cd Images_zmaps_thr
    done
done
